<% if @nw_analytics_omniture_host -%>
omniture.ftp.hostname = <%= @nw_analytics_omniture_host %>
<% else -%>
omniture.ftp.hostname = ftp2.omniture.com
<% end -%>
<% if @nw_analytics_omniture_user -%>
omniture.ftp.username = <%= @nw_analytics_omniture_user %>
<% else -%>
omniture.ftp.username = test1
<% end -%>
<% if @nw_analytics_omniture_pass -%>
omniture.ftp.password = <%= @nw_analytics_omniture_pass %>
<% else -%>
omniture.ftp.password = test1
<% end -%>

<% if @nw_analytics_aws_access_key -%>
amazon.aws.accessKey = <%= @nw_analytics_aws_access_key %>
<% else -%>
amazon.aws.accessKey = AAAAAAAAAAAAAAAAAAAAAAAAAAA
<% end -%>
<% if @nw_analytics_aws_secret_key -%>
amazon.aws.secretKey = <%= @nw_analytics_aws_secret_key %>
<% else -%>
amazon.aws.secretKey = SSSSSSSSSSGGGGGGGGG333333333GGGGGGGGG
<% end -%>

# ===================================================================================================

amazon.aws.s3.dataBucket = nwdb-analytics-data
amazon.aws.s3.rawWebLogsFolder = weblogs/rawByDay
amazon.aws.s3.filteredWebLogsFolder = weblogs/filtered/type3
amazon.aws.sns.topicARN = arn:aws:sns:us-east-1:534885667811:OmnitureDataCopy

recommendation.s3.hiveScriptLocation = s3://nwdb-analytics-util/hive/runAll
recommendation.s3.recommendedLinksTableLocation = s3://nwdb-analytics-data/generated/RecommendedLinks
recommendation.s3.javascriptFilePath = s3://nwdb-analytics-public/recommendation/RecommendedLinks.js
