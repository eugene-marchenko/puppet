# Whether to start ActiveMQ (as a daemon or not)
<% if @enabled =~ /false|no/ -%>
NO_START=1
<% else -%>
NO_START=0
<% end -%>

# Use Debian create user to run ActiveMQ process
<% if @user -%>
ACTIVEMQ_USER="<%= @user %>"
<% else -%>
ACTIVEMQ_USER="activemq"
<% end -%>

# Time to wait for the server to die, in seconds
<% if @dietime -%>
DIETIME=<%= @dietime %>
<% else -%>
DIETIME=2
<% end -%>
